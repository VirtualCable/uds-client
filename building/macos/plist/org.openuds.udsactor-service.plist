<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.openuds.udsactor-service</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/udsactor-service</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>
    <key>StandardOutPath</key>
    <string>/var/log/udsactor-service.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/udsactor-service.err</string>
</dict>
</plist>
